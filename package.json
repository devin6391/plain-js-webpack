    "babel-preset-es2015": "^6.22.0",
    "compression-webpack-plugin": "^0.3.2",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "del-cli": "^0.2.1",
    "ejs": "^2.5.6",
    "ejs-loader": "^0.3.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.10.0",
