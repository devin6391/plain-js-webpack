{
  "name": "plain-js-webpack-babel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean:dist && webpack --config webpack.prod.js --progress --profile && npm run compress:html && del images",
    "clean:dist": "del dist",
    "serve:dev": "webpack-dev-server --config ./webpack.dev.js --progress --profile --watch --content-base src/ --inline --hot",
    "start": "concurrently -k \"npm run serve:dev\" \"npm run serve:mock\" ",
    "serve:prod": "http-serve dist -g",
    "serve:mock": "json-server --watch mock-data.json --routes mock-routes.json --port 3004",
    "compress:html": "gzipme dist/index.html"
  },
  "author": "",
  "license": "ISC",
